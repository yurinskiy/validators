{
  "name": "yurinskiy/validators",
  "description": "Different validators for the validator component.",
  "keywords": [
    "symfony",
    "validator"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Yuriy Yurinskiy",
      "email": "yuriyyurinskiy@yandex.ru"
    }
  ],
  "require": {
    "php": ">=8.1",
    "symfony/validator": "^6.4|^7.0|^8.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0",
    "phpstan/phpstan": "^1.10",
    "phpstan/phpstan-phpunit": "^1.1",
    "phpstan/phpstan-symfony": "^1.2",
    "symfony/phpunit-bridge": "^6.4|^7.0|^8.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Yurinskiy\\Validator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Yurinskiy\\Validator\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "cs-fixer": "./vendor/bin/php-cs-fixer fix -v",
    "code-style": [
      "@cs-fixer",
      "@phpstan"
    ],
    "phpstan": "./vendor/bin/phpstan analyse --memory-limit=512M",
    "test": "./vendor/bin/simple-phpunit"
  }
}